<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:background="@drawable/myshape"
    android:gravity="center"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/layoutRowMain"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="8dip"
        android:background="@drawable/myshape"
        android:orientation="vertical" >

	


        <LinearLayout
            android:id="@+id/layoutRowQuotes"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_margin="8dip"
            android:background="@drawable/myshape"
            android:gravity="center"
            android:orientation="horizontal" >

            <ImageButton
                android:id="@+id/imageQuotes"
                style="@android:style/TextAppearance.Medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@android:color/transparent"
                android:clickable="true"
                android:contentDescription="@string/delete_multiple_phone_favorites"
                android:src="@drawable/quotes_logo_small" >
            </ImageButton>

            <Button
                android:id="@+id/textViewQuotes"
                style="?android:attr/borderlessButtonStyle"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:background="?android:attr/listChoiceBackgroundIndicator"
                android:clickable="true"
                android:gravity="center"
                android:text="@string/label_quotes"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#000000" />
        </LinearLayout>
<TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
    
        android:text="MultiFeelingMode"
        android:textStyle="bold"
         />
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray" />

        <LinearLayout
            android:id="@+id/layoutMapFunctions"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_margin="8dip"
            android:background="@drawable/myshape"
            android:gravity="center"
            android:orientation="horizontal" >

            <ImageButton
                android:id="@+id/imageHomePicture"
                style="@android:style/TextAppearance.Medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@android:color/transparent"
                android:clickable="true"
                android:contentDescription="@string/delete_multiple_phone_favorites"
                android:src="@drawable/home_logo_small" >
            </ImageButton>

           <LinearLayout
                android:id="@+id/imageHomePictureSub"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@drawable/myshape"
                android:gravity="center"
                android:orientation="vertical" >

                <Button
                    android:id="@+id/textViewTakeMeHome"
                    style="?android:attr/borderlessButtonStyle"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginBottom="5dp"
                    android:background="?android:attr/listChoiceBackgroundIndicator"
                    android:clickable="true"
                    android:gravity="center"
                    android:text="@string/setting_take_me_home"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#000000" />

                
            </LinearLayout>
        </LinearLayout>
<TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/contacts2"
        android:text="MultiNavigation"
        android:textStyle="bold"
         />
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray" />

        <LinearLayout
            android:id="@+id/layoutFavoriteContacts"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_margin="8dip"
            android:background="@drawable/myshape"
            android:gravity="center"
            android:orientation="horizontal" >

            <ImageButton
                android:id="@+id/imageLoadFavoriteContacts"
                style="@android:style/TextAppearance.Medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@android:color/transparent"
                android:clickable="true"
                android:contentDescription="@string/delete_multiple_phone_favorites"
                android:src="@drawable/friends_logo_small" >
            </ImageButton>

            <LinearLayout
                android:id="@+id/imageHomePictureSub1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@drawable/myshape"
                android:gravity="center"
                android:orientation="vertical" >

                <Button
                    android:id="@+id/textViewEmergencyContacts"
                    style="?android:attr/borderlessButtonStyle"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginBottom="5dp"
                    android:background="?android:attr/listChoiceBackgroundIndicator"
                    android:clickable="true"
                    android:gravity="center"
                    android:text="@string/emergency_contacts"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#000000" />
            </LinearLayout>
        </LinearLayout>
<TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/contacts"
        android:text="MultiEmergency"
        android:textStyle="bold"
         />
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray" />

        <LinearLayout
            android:id="@+id/layoutDrunkMode"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_margin="8dip"
            android:background="@drawable/myshape"
            android:gravity="center"
            android:orientation="horizontal" >

            <ImageButton
                android:id="@+id/imageDrunkMode"
                style="@android:style/TextAppearance.Medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@android:color/transparent"
                android:clickable="true"
                android:contentDescription="@string/delete_multiple_phone_favorites"
                android:src="@drawable/drunk" >
            </ImageButton>

            <LinearLayout
                android:id="@+id/imageDrunkModeSub"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@drawable/myshape"
                android:gravity="center"
                android:orientation="vertical" >

                <Button
                    android:id="@+id/buttonDrunkMode"
                    style="?android:attr/borderlessButtonStyle"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginBottom="5dp"
                    android:background="?android:attr/listChoiceBackgroundIndicator"
                    android:clickable="true"
                    android:gravity="center"
                    android:text="@string/button_disable_drunk"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#000000" />
            </LinearLayout>
        </LinearLayout>
        <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/contacts4"
        android:text="MultiDrunkMode"
        android:textStyle="bold"
         />
    </LinearLayout>

</LinearLayout><!-- From: file:/C:/android-AppUsageStatistics-master/Multicare/app/src/main/res/layout/activity_main_drunk.xml -->